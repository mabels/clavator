    "awesome-typescript-loader": "3.3.0",
    "chai": ">=4.1.2",
    "css-loader": ">=0.28.7",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": ">=1.1.4",
    "globby": "7.0.0",
    "html-webpack-plugin": ">=2.30.1",
    "img-loader": ">=1.3.1",
    "in-publish": ">=2.0.0",
    "jsdom": "11.4.0",
    "less": "2.7.3",
    "less-loader": ">=4.0.5",
    "mocha": ">=3.5.3",
    "node-jsdom": "3.1.5",
    "nodemon": ">=1.12.1",
    "sinon": "4.0.1",
    "source-map-support": ">=0.4.18",
    "style-loader": "0.19.0",
    "ts-loader": "2.3.7",
    "tslint": ">=5.7.0",
    "typescript": "2.6.1",
    "url-loader": ">=0.5.9",
    "webpack": "3.8.1",
    "webpack-shell-plugin": "0.5.0"
