{
  "name": "clavator",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "compile": "tsc",
    "rollup": "rollup -c rollup.config.js",
    "webpack": "webpack",
    "generator": "node -e 'require(\"./src/server/dispatcher/dice-ware-generator\").generator()'",
    "build": "npm run webpack",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "start": "nodemon ./dist/server.js",
    "pretest": "npm run build",
    "test": "mocha --require ./test/setup ./dist/test",
    "lint": "tslint --project .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/clavator"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/clavator/issues"
  },
  "homepage": "https://github.com/mabels/clavator#readme",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/classnames": "2.2.7",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/express": "4.16.0",
    "@types/fs-extra": "5.0.4",
    "@types/mocha": "5.2.5",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "@types/react-copy-to-clipboard": "4.2.6",
    "@types/react-modal": "3.8.0",
    "@types/react-tabs": "2.3.0",
    "@types/rimraf": "2.0.2",
    "@types/sinon": "7.0.3",
    "@types/uuid": "3.4.3",
    "@types/ws": "3.0.2",
    "@types/yargs": "8.0.2",
    "chai": "4.2.0",
    "enzyme": "3.8.0",
    "globby": "9.0.0",
    "jsdom": "13.1.0",
    "less": "3.9.0",
    "mocha": "5.2.0",
    "mocha-steps": "1.3.0",
    "sinon": "7.2.2",
    "tslint": "5.12.1",
    "typescript": "3.2.2",
    "webpack": "4.28.4",
    "image-loader": "0.0.1",
    "file-loader": "3.0.1",
    "css-loader": "2.1.0",
    "img-loader": "3.0.1",
    "webpack-cli": "3.2.1",
    "cached-request": "1.1.4",
    "@types/request": "2.48.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-plugin": "3.2.0",
    "url-loader": "1.1.2",
    "less-loader": "4.1.0"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "enzyme-adapter-react-16": "1.7.1",
    "express": "4.16.1",
    "font-awesome": "4.7.0",
    "fs-extra": "7.0.1",
    "linebyline": "1.3.0",
    "mobx": "5.8.0",
    "mobx-react": "5.4.3",
    "prettier": "1.15.3",
    "react": "16.7.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.7.0",
    "react-modal": "3.8.1",
    "react-tabs": "3.0.0",
    "request": "2.88.0",
    "rimraf": "2.6.3",
    "ts-loader": "5.3.3",
    "uuid": "3.3.2",
    "winston": "3.1.0",
    "ws": "6.1.2",
    "yargs": "12.0.5"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticPrefix": "chore:",
    "devDependencies": {
      "automerge": true
    },
    "packageRules": [
      {
        "packagePatterns": [
          "@types/"
        ],
        "automerge": true
      }
    ]
  },
  "resolutions": {
    "merge": ">=1.2.1"
  }
}
