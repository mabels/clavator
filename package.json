{
  "name": "clavator",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "engines": {
    "node": ">=6.9.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "rollup": "",
    "compile": "tsc",
    "build": "npm run build && npm run rollup",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "start": "nodemon ./dist/server.js",
    "pretest": "npm run build",
    "test": "mocha --require ./test/setup ./dist/test",
    "lint": "tslint --project .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/clavator"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/clavator/issues"
  },
  "homepage": "https://github.com/mabels/clavator#readme",
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/classnames": "2.2.7",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/express": "4.16.0",
    "@types/fs-extra": "5.0.4",
    "@types/mocha": "5.2.5",
    "@types/react": "16.7.18",
    "@types/react-copy-to-clipboard": "4.2.6",
    "@types/react-dom": "16.0.11",
    "@types/react-modal": "3.8.0",
    "@types/react-tabs": "2.3.0",
    "@types/rimraf": "2.0.2",
    "@types/sinon": "7.0.3",
    "@types/uuid": "3.4.3",
    "@types/ws": "3.0.2",
    "@types/yargs": "8.0.2",
    "chai": "4.2.0",
    "enzyme": "3.8.0",
    "globby": "9.0.0",
    "jsdom": "13.1.0",
    "less": "3.9.0",
    "mocha": "5.2.0",
    "mocha-steps": "1.3.0",
    "rollup": "1.1.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-image": "1.0.2",
    "rollup-plugin-less": "0.1.5",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-uglify": "6.0.1",
    "sinon": "7.2.2",
    "tslint": "5.12.1",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "cached-request": "1.1.4",
    "classnames": "2.2.6",
    "enzyme-adapter-react-16": "1.7.1",
    "express": "4.16.1",
    "font-awesome": "4.7.0",
    "fs-extra": "7.0.1",
    "linebyline": "1.3.0",
    "mobx": "5.8.0",
    "mobx-react": "5.4.3",
    "react": "16.7.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-dom": "16.7.0",
    "react-modal": "3.8.1",
    "react-tabs": "3.0.0",
    "request": "2.88.0",
    "rimraf": "2.6.3",
    "rollup-plugin-filesize": "6.0.0",
