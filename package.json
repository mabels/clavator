{
  "name": "clavator",
  "version": "0.1.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "webpack",
    "build:watch": "webpack --watch --debug --output-pathinfo --progress",
    "start": "nodemon ./dist/server.js",
    "pretest": "npm run build",
    "test": "mocha --require ./test/setup dist/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'"
  },
  "repository": {
    "type": "git",
    "url": "mabels/clavator"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/mabels/clavator/issues"
  },
  "homepage": "https://github.com/mabels/clavator#readme",
  "devDependencies": {
    "awesome-typescript-loader": ">=3.0.0",
    "@types/chai": ">=3.4.33",
    "chai": ">=3.5.0",
    "css-loader": ">=0.23.1",
    "cz-conventional-changelog": ">=1.1.6",
    "@types/enzyme": ">=2.5.38",
    "enzyme": ">=2.3.0",
    "extract-text-webpack-plugin": ">=2.0.0-rc.3",
    "file-loader": ">=0.9.0",
    "globby": ">=5.0.0",
    "html-webpack-plugin": ">=2.24.0",
    "img-loader": ">=1.3.1",
    "in-publish": ">=2.0.0",
    "jsdom": ">=9.4.0",
    "less": ">=2.7.1",
    "less-loader": ">=2.2.3",
    "@types/mocha": ">=2.2.33",
    "mocha": ">=2.5.3",
    "nodemon": ">=1.9.2",
    "react-addons-test-utils": ">=15.1.0",
    "source-map-support": ">=0.4.1",
    "style-loader": ">=0.13.1",
    "tslint": ">=3.11.0",
    "typescript": ">=2.1.5",
    "url-loader": ">=0.5.7",
    "webpack": ">=2.2.0"
  },
  "dependencies": {
    "@types/rimraf": ">=0.0.28",
    "rimraf": ">=2.5.2",
    "react-copy-to-clipboard": "4.2.3",
    "font-awesome": "^4.4.0",
    "@types/classnames": ">=0.0.32",
    "classnames": ">=2.2.5",
    "@types/express": ">=4.0.34",
    "express": ">=4.14.0",
    "@types/ws": ">=0.0.37",
    "ws": ">=1.1.1",
    "linebyline": ">=1.3.0",
    "@types/node-uuid": ">=0.0.28",
    "node-uuid": ">=1.4.7",
    "@types/react": ">=0.14.35",
    "react": ">=15.4.1",
    "@types/react-dom": ">=0.14.19",
    "react-dom": ">=15.4.1",
    "@types/winston": ">=0.0.32",
    "winston": ">=2.2.0",
    "mobx": ">=3.1.0",
    "mobx-react": ">=4.1.0",
    "fs-promise": ">=1.0.0",
    "@types/fs-promise": ">=1.0.1"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
